{
  "extends": "airbnb",
  "rules": {
    "jsx-quotes": 0,
    "react/jsx-curly-spacing": 0,
    "react/no-find-dom-node": 0,
    "react/no-string-refs": 0,
    "react/prop-types": 0,
    "comma-dangle": 0,
    "arrow-parens": 0,
    "template-curly-spacing": 0,
    "no-underscore-dangle": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "no-nested-ternary": 0
  }
}